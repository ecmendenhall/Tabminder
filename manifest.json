{
  "name": "Tabminder",
  "version": "0.9.1",
  "background": { "scripts": ["background.js"] },
  "options_page": "options.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
       ],
  "browser_action": {
    "default_icon": "img/off.png",
    "icons": ["img/off.png", "img/on.png"],
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
