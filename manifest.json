{
  "name": "Tabminder",
  "version": "1.0",
  "background": { "scripts": ["background.js"] },
  "options_page": "options.html",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
       ],
  "browser_action": {
    "default_icon": "off.png",
    "icons": ["off.png", "on.png"],
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
